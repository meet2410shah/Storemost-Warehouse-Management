<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/css/AdminWarehouseList.css">
  </head>
  <body>


    <div class = "topBar">
        <img src = "/images/sort.svg" class ="topBar-openbtn" onclick="openNav()"/>
        <img src = "/images/SM-Warehouse-Logo.png" class ="topBar-storeMostLogo"/>
    </div>



    <div class = "sideBar">
            <div class = "sideLogo">
              <img src = "/images/image.png" class = "buttonclass person-image" />
            </div>
            <div class = "sideLogo">
                <img src = "/images/inventory.svg" class = "buttonclass" />
            </div>
            <div class = "sideLogo">
                <img src = "/images/logout.svg" class = "buttonclass" />
            </div>
    </div>


    <div id="mySidepanel" class="sidepanel">
      <a href="javascript:void(0)" class="closebtn" onclick="closeNav()"
        >&times;</a
      >
      <div class="sideBox">
        <img src="/images/image.png" alt="User Logo" class="userLogo">
        <p class="userName">Abcd Efgh</p>
        <p class="userDesig">Admin</p>

        <table class="table">
          <tbody>
            <tr>
              <td class="leftBox"><img src="/images/image.png" alt="" class="userOptionProfile"></td>
              <td class="rightBox adjust"><p>Profile</p></td>
            </tr>
            <tr>
              <td class="leftBox"><img src="/images/inventory.svg" alt="" class="userOption"></td>
              <td class="rightBox"><p>Warehouses</p></td>
            </tr>
            <tr>
              <td class="leftBox"><img src="/images/logout.svg" alt="" class="userOption"></td>
              <td class="rightBox"><p>Logout</p></td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>








    <div class = "mainBackground">






      <div class = "searchdiv">
          <input type = "text" class = "input"/>
      </div>
      <div class = "buttendiv">
          <button class = "btnsort">
              Filter<img src = "/images/filter.svg" class ="filterimg"/>
          </button>
          <button class = "btnsort">
              Sort<img src = "/images/sort.svg" class ="filterimg"/>
          </button>
      </div>


      <div class="wareList">


        <% AdminWarehouseList.forEach(function(ware) { %>



        <div class = "admin-name-main-conta">
             <div class = "warehouse-img">
             </div>
             <div class = "admin-warehouse-details-div">
             <div class = "admin-flex-1st-div">
                 <h1 class="admin-link">Warehouse <%= ware.warehouseId %></h1>
                 <p class = "admin-occupied"> <%= ware.occupied %> occupied</p>
            </div>
            <div class = "admin-flex-2nd-div">
                 <h2 class = "admin-addr"> Address: </h2>
                 <h2 class = "admin-addr2"> <%= ware.address %> </h2>
            </div>
        </div>
        </div>


        <% }); %>


